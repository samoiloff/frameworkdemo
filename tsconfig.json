{
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/phaser/types",
      "./node_modules/@types"
    ],
    "types": [
      "Phaser",
      "node"
    ],
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es5",
      "dom",
      "es2015.promise",
      "es2015",
      "es2017"
    ],
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": false,
    "noEmitHelpers": true,
    "importHelpers": true,
    "noImplicitAny": false,
    "esModuleInterop": true
  },
  "files": [
    "typings/tsd.d.ts",
    "typings/require.d.ts"
  ],
  "awesomeTypescriptLoaderOptions": {
    "usePrecompiledFiles": false
  }
}